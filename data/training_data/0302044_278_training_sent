\,subsection,{,Order,$\lambda^3$,},We,start,from,\,begin,{,equation,},\,V,_,8,^,{,(,3,),},(,p,_,1,",",\cdots,",",p,_,8,;,m^2,),=,"\frac{1 -
K(p_1+p_2+p_3)}{(p_1+p_2+p_3)^2 + m^2}",\cdot,"\frac{1 - K(p_4 + p_5 +
p_6)}{(p_4+p_5+p_6)^2+m^2}",+,\,text,{,2,7,9,permutations,},\,end,{,equation,},By,a,straightforward,enumeration,of,diagrams,",",we,obtain,\,begin,{,e,q,n,array,},&,&,A,_,8,^,{,(,3,),},(,q,",",-,q,",",q,',",",-,q,',",",0,",",0,",",0,",",0,),=,1,8,\le,f,t,\lbrace,\le,f,t,(,\frac{1-K(q)}{q^2},\,right,),^,2,+,\le,f,t,(,\frac{1-K(q')}{q'^2},\,right,),^,2,\,right,\rbrace,\nonumber,\,\,&,&,+,2,4,\le,f,t,(,\frac{1 - K(q)}{q^2},+,\frac{1 - K(q')}{q'^2},\,right,),\le,f,t,(,\frac{1 - K(q+q')}{(q+q')^2},+,\frac{1 - K(q-q')}{(q-q')^2},\,right,),\nonumber,\,\,&,&,+,2,4,\frac{1-K(q)}{q^2},\frac{1-K(q')}{q'^2},+,1,2,\le,f,t,(,\frac{1-K(q+q')}{(q+q')^2},\,right,),^,2,+,1,2,\le,f,t,(,\frac{1-K(q-q')}{(q-q')^2},\,right,),^,2,\,end,{,e,q,n,array,},Substituting,this,into,the,integral,equation,for,"$A_6^{(3)}(p,-p,0,0,0,0)$",",",we,eventually,obtain,\,begin,{,e,q,n,array,},&,&,A_6,^,{,(,3,),},(,p,",",-,p,",",0,",",0,",",0,",",0,),=,1,2,\,B,i,g,g,[,\,",",\int,_,q,\frac{1-K(q)}{q^2},\le,f,t,(,\frac{1-K(p+q)}{(p+q)^2},\,right,),^,2,\nonumber,\,\,&,&,\,quad,+,"\frac{1 -
K(p)}{p^2}",\int,_,q,\frac{1 - K(q)}{q^2},\le,f,t,(,"\frac{1 -
K(p+q)}{(p+q)^2}",-,\frac{1 - K(q')}{q'^2},\,right,),\,",",\,B,i,g,g,],\,end,{,e,q,n,array,},In,getting,this,result,",",the,integral,over,the,logarithmic,scale,parameter,$t$,has,been,done,by,rewriting,the,integrand,as,much,as,possible,as,a,total,derivative,with,respect,to,$t$,.
